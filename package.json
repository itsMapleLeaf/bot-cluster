{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel --print-label --race dev-bots lavalink",
    "dev-bots": "nodemon --exec esmo --no-warnings --enable-source-maps --inspect=5858 src/main.ts",
    "start": "npm-run-all --parallel --print-label --race start-bots lavalink",
    "start-bots": "esmo --no-warnings src/main.ts",
    "typecheck": "tsc --noEmit",
    "update-heroku-config": "esmo --no-warnings scripts/update-heroku-config.ts",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "lavalink": "java -jar Lavalink.jar",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@discordjs/voice": "^0.7.5",
    "@itsmapleleaf/gatekeeper": "^0.9.0",
    "chalk": "^4",
    "concurrently": "^6.4.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "execa": "^6.0.0",
    "googleapis": "^92.0.0",
    "libsodium-wrappers": "^0.7.9",
    "mobx": "^6.3.8",
    "opusscript": "^0.0.8",
    "pretty-ms": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "ws": "^8.3.0",
    "youtubei": "^0.0.1-rc.27",
    "zod": "^3.11.6",
    "esno": "^0.12.1",
    "@itsmapleleaf/configs": "^1.0.4",
    "node-fetch": "^3.1.0",
    "@lavaclient/types": "^2.0.11"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "depcheck": "^1.4.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "type-fest": "^2.6.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": "^16 || ^17"
  },
  "prettier": "@itsmapleleaf/configs/prettier",
  "eslintConfig": {
    "extends": [
      "./node_modules/@itsmapleleaf/configs/eslint"
    ],
    "rules": {
      "import/no-unused-modules": [
        "warn",
        {
          "unusedExports": true,
          "ignoreExports": [
            "src/**/commands/**/*"
          ]
        }
      ]
    }
  }
}
