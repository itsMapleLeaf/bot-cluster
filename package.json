{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all dev-bots lavalink",
    "dev-bots": "nodemon --exec esmo --no-warnings --enable-source-maps --inspect=5858 src/main.ts",
    "start": "concurrently 'esmo --no-warnings src/main.ts' 'pnpm lavalink'",
    "typecheck": "tsc --noEmit",
    "update-heroku-config": "esmo --no-warnings scripts/update-heroku-config.ts",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "lavalink": "java -jar Lavalink.jar"
  },
  "dependencies": {
    "@discordjs/voice": "^0.7.5",
    "@itsmapleleaf/gatekeeper": "^0.9.0",
    "chalk": "^4",
    "concurrently": "^6.4.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "erela.js": "^2.3.3",
    "execa": "^6.0.0",
    "googleapis": "^92.0.0",
    "lavaclient": "^4.0.5",
    "libsodium-wrappers": "^0.7.9",
    "mobx": "^6.3.8",
    "opusscript": "^0.0.8",
    "pretty-ms": "^7.0.1",
    "ws": "^8.3.0",
    "youtube.ts": "^0.2.2",
    "youtubei": "^0.0.1-rc.27",
    "ytdl-core": "^4.9.1",
    "ytdl-core-discord": "^1.3.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.0.3",
    "@lavaclient/types": "^2.0.11",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "discord-api-types": "^0.25.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "esno": "^0.12.1",
    "node-fetch": "^3.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "type-fest": "^2.6.0",
    "typescript": "^4.6.0-dev.20211130"
  },
  "engines": {
    "node": "^16 || ^17"
  },
  "prettier": "@itsmapleleaf/configs/prettier",
  "eslintConfig": {
    "extends": [
      "./node_modules/@itsmapleleaf/configs/eslint"
    ],
    "rules": {
      "require-await": "warn"
    }
  }
}
